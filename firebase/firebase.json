{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/auth/github",
        "function": "githubAuth"
      },
      {
        "source": "/api/auth/github/callback",
        "function": "githubCallback"
      },
      {
        "source": "/api/user/profile",
        "function": "getUserProfile"
      },
      {
        "source": "/health",
        "function": "health"
      }
    ]
  }
}