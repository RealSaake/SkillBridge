# SkillBridge Frontend Production Environment Variables
# Copy this file to .env.production and fill in your values

# API Configuration
REACT_APP_API_URL=https://your-api-domain.railway.app
REACT_APP_ENVIRONMENT=production

# GitHub OAuth Configuration
REACT_APP_GITHUB_CLIENT_ID=your_github_oauth_client_id

# Optional: Analytics and Monitoring
REACT_APP_SENTRY_DSN=your_sentry_dsn_for_error_tracking
REACT_APP_GA_TRACKING_ID=your_google_analytics_id

# Optional: Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true